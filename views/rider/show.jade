extends layout
block babysitters
    h1.
        #{title}
    .col-xs-12.col-md-8
        .alert.alert-success(role='alert')
            | Successfully updated the details!
        table.table.table-striped.table-hover
            tr
                td Name
                td= blob.name
            tr
                td Address
                td= blob.address
            tr
                td Phone
                td= blob.phone
            tr
                td Date
                td= blob.dateEnrolled
            tr
                td Locality
                td= blob.locality
            tr
                td Categories
                td= blob.categories
            tr
                td Is Verified?
                td= blob.isVerified
            tr
                td DOB
                td= blob.dob